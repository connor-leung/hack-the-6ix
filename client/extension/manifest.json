{
    "manifest_version": 3,
    "name": "Scholarship Application Reviewer",
    "version": "1.0",
    "description": "A Chrome extension to review scholarship applications and provide suggestions.",
    "permissions": ["storage", "activeTab", "scripting"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {

      }
    },
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "icons": {

    }
  }
  